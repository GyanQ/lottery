<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.vietnam.lottery.business.grabRedPacketsDetail.mapper.GrabRedPacketsDetailMapper">

    <select id="lotteryAmount"
            resultType="com.vietnam.lottery.business.sysUser.response.LotteryAmountResponse">
        select sum(amount) as amount,
               id          as id
        from lottery_detail
        where create_by = #{userId}
    </select>
</mapper>

