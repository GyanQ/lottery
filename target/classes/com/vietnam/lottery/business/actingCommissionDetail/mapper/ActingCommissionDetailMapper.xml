<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.vietnam.lottery.business.actingCommissionDetail.mapper.ActingCommissionDetailMapper">

    <select id="commissionAmount" resultType="com.vietnam.lottery.business.sysUser.response.CommissionAmountResponse">
        select id                     as id,
               ifnull(sum(amount), 0) as amount
        from acting_commission_detail
        where create_by = #{userId}
    </select>
</mapper>

